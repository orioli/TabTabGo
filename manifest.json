{
  "manifest_version": 3,
  "name": "TabTabGo - AI TrackPad NU - Berengueres (SEDS) & Guirao & Dina Kalinina (NUSOM)",
  "version": "1.0.3",
  "description": "Intercepts Tab or D key to cycle through prominent buttons on the page",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "TabTabGo - Smart Button Navigation"
  },
  "content_scripts": [
    {

      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ]
}

